{
  "view": {
    "prefix": "view",
    "body": [
    "<view>",
    "\t",
    "</view>"
    ],
    "description": "视图容器",
    "scope": "source.scheme"
  },
  "scroll-view": {
    "prefix": "scroll-view",
    "body": [
      "<scroll-view scroll-${1:x}=\"$2\">",
      "\t$3",
      "</scroll-view>$4"
    ],
    "description": "可滚动视图区域"
  },
  "swiper": {
    "prefix": "swiper",
    "body": [
      "<swiper indicator-dots=\"{{${1:indicatorDots}}}\" autoplay=\"{{${2:autoplay}}}\" interval=\"{{${3:interval}}}\" duration=\"{{${4:duration}}}\">",
      "\t<block tt:for=\"{{${5:imgUrls}}}\">",
      "\t\t<swiper-item>",
      "\t\t\t<image src=\"{{${6:item}}}\" class=\"slide-image\" />",
      "\t\t</swiper-item>",
      "\t</block>",
      "</swiper>$7"
    ],
    "description": "滑块视图容器"
  },
  "icon": {
    "prefix": "icon",
    "body": "<icon type=\"$1\" size=\"$2\" color=\"$3\" />$4",
    "description": "图标"
  },
  "text": {
    "prefix": "text",
    "body": "<text>$1</text>$2",
    "description": "文本"
  },
  "textarea":{
    "prefix": "textarea",
    "body": "<textarea>$1</textarea>$2",
    "description": "多行文本"
  },
  "rich-text": {
    "prefix": "rich-text",
    "body": "<rich-text nodes=\"$1\"></rich-text>"
  },
  "progress": {
    "prefix": "progress",
    "body": "<progress percent=\"$1\" color=\"$2\" stroke-width=\"${3:6}\" active-color=\"${4:#F85959}\" background-color=\"${5:#EBEBEB}\" ${6:active} />",
    "description": "进度条"
  },
  "button": {
    "prefix": "button",
    "body": "<button type=\"${1:default}\" size=\"${2:defaultSize}\" loading=\"${3:loading}\" disabled=\"${5:disabled}\" hover-class=\"${6:other-button-hover}\"> Button </button>",
    "description": "按钮"
  },
  "checkbox-group": {
    "prefix": "checkboxGroup",
    "body": [
      "<checkbox-group>",
      "\t<label tt:for=\"{{${1:item}}}\" tt:key=\"{{${1:item}${2:value}}}\">",
      "\t\t<checkbox value=\"${1:item}.${3:name}\" />{{${1:item}.${4:value}}}",
      "\t</label>",
      "</checkbox-group>$5"
    ],
    "description": "多项checkbox选择器"
  },
  "radio-group": {
    "prefix": "radioGroup",
    "body": [
      "<radio-group>",
      "\t<label class=\"$1\" :for=\"{{${2:item}}}\">",
      "\t\t<view class=\"\">",
      "\t\t\t<radio value=\"${2:item}.${3:name}\" checked=\"{{${2:item}.${4:checked}}}\" />{{${2:item}.${5:value}}}",
      "\t\t</view>",
      "\t</label>",
      "</radio-group>$6"
    ],
    "description": "多项radio选择器"
  },
  "form": {
    "prefix": "form",
    "body": "<form catchsubmit=\"$1\" catchreset=\"$2\">$3</form>$4",
    "description": "表单"
  },
  "input": {
    "prefix": "input",
    "body": "<input type=\"$1\" placeholder=\"$2\" ${3:focus} ${4:password} />$5",
    "description": "自动聚焦的input"
  },
  "label": {
    "prefix": "label",
    "body": "<label for=\"$1\">$2</label>$3",
    "description": "label"
  },
  "picker-time": {
    "prefix": "pickerTime",
    "body": [
      "<picker mode=\"time\" start=\"${1:xx:xx}\" end=\"${2:xx:xx}\">$3</picker>$4"
    ],
    "description": "时间选择器"
  },
  "picker-date": {
    "prefix": "pickerDate",
    "body": [
      "<picker mode=\"date\" start=\"${1:xxxx-xx-xx}\" end=\"${2:xxxx-xx-xx}\">$3</picker>$4"
    ],
    "description": "日期选择器"
  },
  "picker-view": {
    "prefix": "pickerView",
    "body": [
      "<picker-view value=\"{{${1:[]}}}\" bindchange=\"${2:\"\"}\">",
      "\t<picker-view-column>",
      "\t\t",
      "\t</picker-view-column>",
      "\t<picker-view-column>",
      "\t\t",
      "\t</picker-view-column>",
      "\t<picker-view-column>",
      "\t\t",
      "\t</picker-view-column>",
      "</picker-view>"
    ],
    "description": "嵌入页面的滚动选择器"
  },
  "slider": {
    "prefix": "slider",
    "body": "<slider min=\"$1\" max=\"$2\" step=\"$3\" />$4",
    "description": "滑动选择器"
  },
  "switch": {
    "prefix": "switch",
    "body": "<switch checked=\"${1:false}\" />$2",
    "description": "开关选择器"
  },
  "actoin-sheet": {
    "prefix": "actionSheet",
    "body": [
      "<action-sheet>",
      "\t<block tt:for=\"{{${1:actionSheetItems}}}\">",
      "\t\t<action-sheet-item class=\"item\" data-name=\"{{${2:item}}}\" />{{${2:item}}}</action-sheet-item>",
      "\t</block>",
      "</action-sheet>$3"
    ],
    "description": "从屏幕底部出现的菜单表"
  },
  "modal": {
    "prefix": "modal",
    "body": "<modal title=\"$1\" confirm-text=\"$2\" cancel-text=\"$3\" bindconfirm=\"$4\" bindcancel=\"$5\">$6</modal>$7",
    "description": "模态弹窗"
  },
  "toast": {
    "prefix": "toast",
    "body": "<toast>$1</toast>$2",
    "description": "消息提示框"
  },
  "web-view": {
    "prefix": "webView",
    "body": "<web-view src=\"\">$1</web-view>$2",
    "description": "承载网页的容器"
  },
  "loading": {
    "prefix": "loading",
    "body": "<loading>$1</loading>$2",
    "description": "加载提示"
  },
  "navigator": {
    "prefix": "navigator",
    "body": "<navigator url=\"$1\" open-type=\"${2:\"redirect\"}\" hover-class=\"${3:className}\">$4</navigator>$5",
    "description": "导航"
  },
  "audio": {
    "prefix": "audio",
    "body": "<audio src=\"$1\" ${2:controls} ${3:loop}></audio>$2",
    "description": "音频"
  },
  "image": {
    "prefix": "image",
    "body": "<image src=\"$1\" mode=\"$2\" />$2",
    "description": "图片"
  },
  "video": {
    "prefix": "video",
    "body": "<video src=\"$1\" ${2:autoplay}></video>$2",
    "description": "视频"
  },
  "map": {
    "prefix": "map",
    "body": "<map longitude=\"$1\" latitude=\"$2\"></map>$3",
    "description": "地图"
  },
  "tt:for": {
    "prefix": "ttfor",
    "body": "tt:for=\"{{${1:items}}}\" tt:for-index=\"$2\" tt:for-item=\"$3\"",
    "description": "列表渲染"
  },
  "tt:if": {
    "prefix": "ttif",
    "body": "tt:if=\"{{condition}}\"",
    "description": "条件渲染if"
  },
  "tt:elif": {
    "prefix": "ttelif",
    "body": "tt:elif=\"{{condition}}\"",
    "description": "条件渲染else if"
  },
  "class": {
    "prefix": "class",
    "body": "class=\"$1\"",
    "description": "类名"
  },
  "id": {
    "prefix": "id",
    "body": "id=\"$1\"",
    "description": "ID名"
  },
  "url": {
    "prefix": "url",
    "body": "url=\"$1\"",
    "description": "url"
  },
  "value": {
    "prefix": "value",
    "body": "value=\"$1\"",
    "description": "value值"
  },
  "placeholder": {
    "prefix": "placeholder",
    "body": "placeholder=\"$1\"",
    "description": "placeholder提示"
  },
  "catchsubmit": {
    "prefix": "catchsubmit",
    "body": "catchsubmit=\"$1\"",
    "description": "catchsubmit"
  },
  "catchreset": {
    "prefix": "catchreset",
    "body": "catchreset=\"$1\"",
    "description": "catchreset"
  },
  "bindchange": {
    "prefix": "bindchange",
    "body": "bindchange=\"$1\"",
    "description": "bindchange"
  },
  "bindtap": {
    "prefix": "bindtap",
    "body": "bindtap=\"${1:tapname}\"",
    "description": "绑定事件"
  },
  "template": {
    "prefix": "template",
    "body": [
      "<template name=\"\">",
      "\t",
      "</template>"
    ],
    "description": "模板"
  },
  "canvas":{
    "prefix": "canvas",
    "body": "<canvas canvas-id=\"$1\" class=\"\"></canvas>",
    "description": "画布"
  }
}
